create = (self, content: @string) {
    return $.http_request(
        "post",
        "https://discord.com/api/v9/guilds",
        self.headers(),
        content
    )
}

get = (self, guild_id: @string) {
    return $.http_request(
        "get",
        "https://discord.com/api/v9/guilds/{}".fmt(guild_id),
        self.headers(),
        ""
    )
}

return {
    create,
    get
}